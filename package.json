{
  "name": "visResumeMook",
  "version": "1.0.0",
  "author": "",
  "type": "module",
  "description": "简历平台桌面应用",
  "main": "./dist/electron.cjs",
  "scripts": {
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "start": "concurrently  'npm:start:render' 'npm:start:main-watch' ",
    "start:main-watch": "nodemon  --exec npm run start:main",
    "start:main": "webpack --config ./config/webpack.main.dev.cjs  --node-env development && electron ./dist/electron.cjs",
    "start:render": "webpack serve --config ./config/webpack.render.dev.cjs  --node-env development",
    "start:web": "webpack serve --config ./config/webpack.web.dev.cjs  --node-env development",
    "build:web": "webpack --config ./config/webpack.web.dev.cjs --node-env production",
    "build:render": "webpack --config ./config/webpack.render.dev.cjs --node-env production",
    "build:main": "webpack --config ./config/webpack.main.dev.cjs --node-env production",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --fix app/",
    "commit": "npx cz"
  },
  "keywords": [],
  "license": "ISC",
  "dependencies": {
    "@douyinfe/semi-icons": "^2.20.4",
    "@douyinfe/semi-ui": "^2.20.4",
    "@reduxjs/toolkit": "^1.8.5",
    "classnames": "^2.3.2",
    "core-js": "^3.25.4",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "markdown-to-jsx": "^7.1.7",
    "path-to-regexp": "^6.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1",
    "regenerator-runtime": "^0.13.9",
    "unist-util-stringify-position": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@douyinfe/semi-foundation": "^2.20.4",
    "@types/node": "^18.8.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vue/preload-webpack-plugin": "^2.0.0",
    "autoprefixer": "^10.4.12",
    "axios": "^1.0.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-transform-default-named-imports": "^1.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.2.5",
    "concurrently": "^7.4.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "cz-conventional-changelog": "^3.3.0",
    "electron": "^21.0.1",
    "electron-builder": "^23.6.0",
    "eslint": "^8.24.0",
    "eslint-config-alloy": "^4.7.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^3.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.0",
    "install": "^0.13.0",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.17",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.7.1",
    "style-loader": "^3.3.1",
    "typescript": "^4.8.4",
    "typescript-plugin-css-modules": "^3.4.0",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "build": {
    "appId": "com.kakachake.resume",
    "productName": "简历平台",
    "copyright": "copyright © 2021 kakachake",
    "directories": {
      "output": "dist"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "mac": {
      "target": "dmg",
      "icon": "build/icon.icns"
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ],
      "icon": "build/icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "build/icon.png"
    }
  }
}
